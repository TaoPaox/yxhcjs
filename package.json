{
  "name": "whale",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "workspaces": [
    "packages/*",
    "play",
    "docs"
  ],
  "scripts": {
    "demo:request": "pnpm run  -C examples/demo-request dev",
    "demo": "pnpm run  -C demo dev",
    "demo:build:prod": "pnpm -C demo build:prod",
    "play": "pnpm run  -C play dev",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "pnpm run -C docs serve",
    "build": "pnpm run -C internal/build start",
    "clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
    "clean:dist": "rimraf dist",
    "build:vite": "vite build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "stub": "pnpm run -r --parallel stub",
    "postinstall": "pnpm stub",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.json --max-warnings 0 && pretty-quick --check --branch dev",
    "lint:fix": "eslint --fix . --ext .vue,.js,.ts,.jsx,.tsx,.json && pretty-quick --branch dev",
    "build:theme": "pnpm run -C packages/theme-chalk build",
    "docs:preview": "vitepress preview docss"
  },
  "dependencies": {
    "@vue/shared": "^3.2.37",
    "@whale/components": "workspace:*",
    "@whale/locale": "workspace:*",
    "@whale/theme-chalk": "workspace:*",
    "@whale/utils": "workspace:*",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@babel/runtime": "^7.24.5",
    "@babel/types": "^7.21.2",
    "@esbuild-kit/cjs-loader": "^2.2.1",
    "@pnpm/find-workspace-packages": "^4.0.16",
    "@pnpm/types": "^8.4.0",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@types/gulp": "^4.0.9",
    "@types/node": "*",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/tsconfig": "^0.1.3",
    "@whale/build": "workspace:*",
    "@whale/build-constants": "workspace:*",
    "@whale/build-utils": "workspace:0.0.1",
    "@whale/eslint-config": "workspace:*",
    "chalk": "^5.0.1",
    "consola": "^2.15.3",
    "eslint": "^8.22.0",
    "eslint-define-config": "^1.5.1",
    "gulp": "^4.0.2",
    "inquirer": "^9.2.22",
    "npm": "^8.18.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.33.1",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.53.0",
    "typescript": "^5.2.2",
    "unplugin-vue-define-options": "^0.6.1",
    "vite": "^3.0.7",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vitepress": "^1.2.2"
  },
  "engines": {
    "node": ">= 16"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,md,json}": "eslint --fix"
  }
}
